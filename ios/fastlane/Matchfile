# Use a private git repo to store certs/profiles
git_url(ENV['MATCH_GIT_URL'])
storage_mode("git")
type("appstore")
branch("main")

# Auth for the private repo (Fine-grained PAT)
git_basic_authorization(ENV['MATCH_GIT_TOKEN'])

# General options
shallow_clone(true)
readonly(false)
force_for_new_devices(false)  # set true only if you rotate dev certs
